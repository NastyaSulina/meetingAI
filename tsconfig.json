{
    "compilerOptions": {
        "baseUrl": "./src",
        "outDir": "./dist/",
        "noImplicitAny": true,
        "target": "es5",
        "module": "esnext",
        "types": ["node", "webpack-env"],
        "jsx": "react",
        "allowJs": true,
        "allowSyntheticDefaultImports": true,
        "moduleResolution": "Node"
    },
    "exclude": ["node_modules", "dist"]
}
